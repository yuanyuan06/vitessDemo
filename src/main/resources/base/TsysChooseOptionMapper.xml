<mapper namespace="io.vitess.dao.sqlmap.TsysChooseOptionDao">
	<resultMap id="TsysChooseOptionResult" type="io.vitess.model.sqlmap.TsysChooseOption">
	</resultMap>
	
	<!-- 用于select查询公用抽取的列 -->
	<sql id="tsysChooseOptionColumns">
	    <![CDATA[
	        	ID as id,
	        	CATEGORY_AVAILABLE as categoryAvailable,
	        	CATEGORY_CODE as categoryCode,
	        	CATEGORY_NAME as categoryName,
	        	IS_AVAILABLE as isAvailable,
	        	OPTION_DESCRIPTION as optionDescription,
	        	OPTION_KEY as optionKey,
	        	OPTION_VALUE as optionValue,
	        	PACKAGE_NAME as packageName,
	        	SORT_NO as sortNo
	    ]]>
	</sql>

		<!-- useGeneratedKeys="true" keyProperty="xxx" for sqlserver and mysql -->
		<insert id="insert" parameterType="io.vitess.model.sqlmap.TsysChooseOption"
				
			 flushCache="true">
		    <![CDATA[
		        INSERT INTO
		        t_sys_choose_option (
		        id,
		        	CATEGORY_AVAILABLE ,
		        	CATEGORY_CODE ,
		        	CATEGORY_NAME ,
		        	IS_AVAILABLE ,
		        	OPTION_DESCRIPTION ,
		        	OPTION_KEY ,
		        	OPTION_VALUE ,
		        	PACKAGE_NAME ,
		        	SORT_NO 
		        ) VALUES (
		        #{id,javaType=java.lang.Long,jdbcType=BIGINT} ,
		        	#{categoryAvailable,javaType=Boolean,jdbcType=BIT} ,
		        	#{categoryCode,javaType=String,jdbcType=VARCHAR} ,
		        	#{categoryName,javaType=String,jdbcType=VARCHAR} ,
		        	#{isAvailable,javaType=Boolean,jdbcType=BIT} ,
		        	#{optionDescription,javaType=String,jdbcType=LONGVARCHAR} ,
		        	#{optionKey,javaType=String,jdbcType=VARCHAR} ,
		        	#{optionValue,javaType=String,jdbcType=VARCHAR} ,
		        	#{packageName,javaType=String,jdbcType=VARCHAR} ,
		        	#{sortNo,javaType=Integer,jdbcType=INTEGER} 
		        )
		    ]]>

		</insert>
		

		<update id="update" parameterType="io.vitess.model.sqlmap.TsysChooseOption">
	    <![CDATA[
	        UPDATE t_sys_choose_option SET
		        CATEGORY_AVAILABLE = #{categoryAvailable,javaType=Boolean,jdbcType=BIT} ,
		        CATEGORY_CODE = #{categoryCode,javaType=String,jdbcType=VARCHAR} ,
		        CATEGORY_NAME = #{categoryName,javaType=String,jdbcType=VARCHAR} ,
		        IS_AVAILABLE = #{isAvailable,javaType=Boolean,jdbcType=BIT} ,
		        OPTION_DESCRIPTION = #{optionDescription,javaType=String,jdbcType=LONGVARCHAR} ,
		        OPTION_KEY = #{optionKey,javaType=String,jdbcType=VARCHAR} ,
		        OPTION_VALUE = #{optionValue,javaType=String,jdbcType=VARCHAR} ,
		        PACKAGE_NAME = #{packageName,javaType=String,jdbcType=VARCHAR} ,
		        SORT_NO = #{sortNo,javaType=Integer,jdbcType=INTEGER} 
	        WHERE 
		        ID = #{id} 
	    ]]>
		</update>
	   <delete id="delete" parameterType="java.lang.Long">
	    <![CDATA[
	        delete from t_sys_choose_option where
		        ID = #{id} 
	    ]]>
	    </delete> 
	    
	     <delete id="deleteByIds" parameterType="list">
	    
	        delete from t_sys_choose_option 
	        where id in
				<foreach item="item" index="index" collection="list" open="(" separator="," close=")">
#{item}				</foreach>	
	  
	    </delete>  
	         
	    <select id="findById" parameterType="java.lang.Long" resultMap="TsysChooseOptionResult" flushCache="false">
			select <include refid="tsysChooseOptionColumns" />
		    <![CDATA[
			    from t_sys_choose_option 
		        where 
		        ID = #{id} 
		    ]]>
		</select>	
		<select id="getByIds" parameterType="list"  resultMap="TsysChooseOptionResult" flushCache="false">
			select <include refid="tsysChooseOptionColumns" />
			    from t_sys_choose_option 
		        where id in
				<foreach item="item" index="index" collection="list" open="(" separator="," close=")">
#{item}				</foreach>					
		</select>	
	<sql id="tsysChooseOptionDynamicWhere">
		<!-- ognl访问静态方法的表达式 为@class@method(args),以下为调用rapid中的Ognl.isNotEmpty()方法,还有其它方法如isNotBlank()可以使用，具体请查看Ognl类 -->
		<where>
	       
				<if test="id!=null">
					and ID = #{id}
				</if>
	       
				<if test="categoryAvailable!=null">
					and CATEGORY_AVAILABLE = #{categoryAvailable}
				</if>
	       
				<if test="categoryCode!=null">
					and CATEGORY_CODE = #{categoryCode}
				</if>
	       
				<if test="categoryName!=null">
					and CATEGORY_NAME = #{categoryName}
				</if>
	       
				<if test="isAvailable!=null">
					and IS_AVAILABLE = #{isAvailable}
				</if>
	       
				<if test="optionDescription!=null">
					and OPTION_DESCRIPTION = #{optionDescription}
				</if>
	       
				<if test="optionKey!=null">
					and OPTION_KEY = #{optionKey}
				</if>
	       
				<if test="optionValue!=null">
					and OPTION_VALUE = #{optionValue}
				</if>
	       
				<if test="packageName!=null">
					and PACKAGE_NAME = #{packageName}
				</if>
	       
				<if test="sortNo!=null">
					and SORT_NO = #{sortNo}
				</if>
		</where>
	</sql>
	

	<select id="getCount" resultType="long">
        select count(*) from t_sys_choose_option 
    </select>
    <select id="findListCountByQueryMap" resultType="long">
        select count(*) from t_sys_choose_option 
		<include refid="tsysChooseOptionDynamicWhere"/>    
    </select>
    
    <select id="findListCountByParam" resultType="long">
        select count(*) from t_sys_choose_option 
		<include refid="tsysChooseOptionDynamicWhere"/>    
    </select>
    
    <!--
    	分页查询已经使用Dialect进行分页,也可以不使用Dialect直接编写分页
    	因为分页查询将传 offset,pageSize,lastRows 三个参数,不同的数据库可以根于此三个参数属性应用不同的分页实现
    -->
    <select id="findListByQueryMapWithPage" resultMap="TsysChooseOptionResult">
    	select <include refid="tsysChooseOptionColumns" />
	    from t_sys_choose_option 
		<include refid="tsysChooseOptionDynamicWhere"/>
		
    </select>
    
    <select id="findListByQueryMap" resultMap="TsysChooseOptionResult">
    	select <include refid="tsysChooseOptionColumns" />
	    from t_sys_choose_option 
		<include refid="tsysChooseOptionDynamicWhere"/>
		
    </select>
    
    <select id="findListByParam" resultMap="TsysChooseOptionResult">
    	select <include refid="tsysChooseOptionColumns" />
	    from t_sys_choose_option 
		<include refid="tsysChooseOptionDynamicWhere"/>
		
    </select>

	
</mapper>
