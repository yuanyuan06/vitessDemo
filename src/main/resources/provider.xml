<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans        http://www.springframework.org/schema/beans/spring-beans.xsd        http://code.alibabatech.com/schema/dubbo        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="example"  />

    <dubbo:registry address="${dubbo.registry.address}"  register="${dubbo.register.flag}"/>

    <dubbo:protocol id="dubbo" name="dubbo" port="${dubbo.app.port}"  threadpool="fixed" threads="800"/>
    <dubbo:provider protocol="dubbo" default="true"  version="${dubbo.default.version}" timeout="350000" threadpool="fixed" threads="800"/>

    <!--拼台销售数据解析-->
    <dubbo:service interface="io.vitess.service.mq.TbTradeParsePorxyManager" ref="tbTradeParsePorxyManager" />
    <!--创单-->
    <dubbo:service interface="io.vitess.service.so.PlatformSoManagerProxy" ref="tbTradeParsePorxyManager" />

</beans>