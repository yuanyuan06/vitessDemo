<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="io.vitess.dao.base.ChooseOptionDao">
    <select id="findListByPackageName" resultType="io.vitess.model.base.ChooseOption">
        select
          *
        from
          t_sys_choose_option o
        where
          o.packageName = #{packageName}

    </select>
</mapper>