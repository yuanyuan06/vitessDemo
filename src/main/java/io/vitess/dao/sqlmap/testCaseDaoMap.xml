<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
        "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">


<mapper namespace="io.vitess.dao.TestCaseDao">
    <!-- useGeneratedKeys="true" keyProperty="xxx" for sqlserver and mysql -->
    <insert id="insert" parameterType="io.vitess.model.TestCase" flushCache="true">
        <![CDATA[
		    insert into test_case (page, time_created_ns, message) values (#{page}, #{timeCreatedNs}, #{message})
		    ]]>
    </insert>
</mapper>